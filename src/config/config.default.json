{
  "Config": {
    "port": 3000,
    "appConfig": {
      "version": "0.0.1",
      "name": "node-simple-server",
      "env": "development"
    },
    "DBConnections": {
      "mongo": {
        "dbName": "node-simple-db",
        "host": "localhost",
        "port": "27017"
      },
      "mysql": {
        "user": "root",
        "password": "",
        "host": "localhost",
        "database": "node-simple-db",
        "options": {
          "requestTimeout": 300000,
          "dialect": "mysql",
          "operatorsAliases": false,
          "logging": true,
          "dialectOptions": {
            "encrypt": false
          }
        }
      }
    }
  }
}
